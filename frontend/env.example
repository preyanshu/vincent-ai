# Frontend Environment Variables
# Copy this file to .env.local and fill in your actual values

# API Endpoints
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_AGENT_SERVER_URL=http://localhost:8000
NEXT_PUBLIC_JOB_RUNNER_URL=http://localhost:3000

# Blockchain Configuration
NEXT_PUBLIC_SEI_NETWORK=sei-testnet
NEXT_PUBLIC_SEI_RPC_URL=https://sei-testnet-rpc.example.com
NEXT_PUBLIC_SEI_EXPLORER_URL=https://sei-testnet-explorer.example.com

# Feature Flags
NEXT_PUBLIC_ENABLE_CHAT=true
NEXT_PUBLIC_ENABLE_BLOCKCHAIN_TOOLS=true
NEXT_PUBLIC_ENABLE_JOB_MONITORING=true

# Analytics (Optional)
NEXT_PUBLIC_ANALYTICS_ID=your_analytics_id_here

# Environment
NODE_ENV=development 