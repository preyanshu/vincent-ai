# Sei.js Environment Variables
# Copy this file to .env and fill in your actual values

# MCP Server Configuration
MCP_SERVER_PORT=8080
MCP_SERVER_HOST=localhost

# Wallet Configuration
WALLET_MODE=disabled
PRIVATE_KEY=your_private_key_here_with_0x_prefix

# Blockchain RPC Endpoints
SEI_MAINNET_RPC=https://sei-rpc.example.com
SEI_TESTNET_RPC=https://sei-testnet-rpc.example.com
SEI_DEVNET_RPC=https://sei-devnet-rpc.example.com

# API Keys (if needed)
SEI_API_KEY=your_sei_api_key_here

# Development Configuration
NODE_ENV=development
LOG_LEVEL=info

# Testing Configuration
TEST_PRIVATE_KEY=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef 